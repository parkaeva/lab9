#include <iostream>
using namespace std;
int main()
{
    int n;
    string c;
    cin>>c>>n;
    setlocale(LC_ALL," ");
    if (c == "С")
    {
        if(n==0)
        {
            cout<<"север";
        } else if(n==1)
        {
            cout<<"восток";
        }else if(n==2)
        {
            cout<<"запад";
        }
    }
    if (c == "З")
    {
        if(n==0)
        {
            cout<<"запад";
            
        } else if(n==1)
        {
            cout<<"юг";
            
        }else if(n==2)
        {
            cout<<"север";
            
        }
        
    }
    if (c == "Ю")
    {
        if(n==0)
        {
            cout<<"юг";
            
        } else if(n==1)
        {
            cout<<"запад";
            
        }else if(n==2)
    {
        cout<<"восток";
    }
    }
    if (c == "В")
    {
        if(n==0)
        {
            cout<<"восток";
            
        } else if(n==1)
        {
            cout<<"юг";
            
        }else if(n==2)
        {
            cout<<"север";
            
        }
        
    }
    return 0;
}
